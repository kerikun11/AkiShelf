  )
  (via_rule
    "kicad_default" "Via[0-1]_600:300_um-kicad_default"
  )
  (via_rule
    3.3V "Via[0-1]_1200:800_um-3.3V"
  )
  (via_rule
    5V "Via[0-1]_1200:800_um-5V"
  )
  (via_rule
    GND "Via[0-1]_1200:800_um-GND"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 300.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    IO0 ENABLE "TENKEY_R4" "TENKEY_C1" "TENKEY_C2" "TENKEY_C3" "TENKEY_R3" "TENKEY_R2"
